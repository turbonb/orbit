{
  "name": "orbit",
  "version": "0.1.0",
  "private": true,
  "description": "Orbit web presence and API monorepo scaffold.",
  "workspaces": [
    "frontend",
    "mcp/google-maps",
    "mcp/google-sheets"
  ],
  "scripts": {
    "dev": "npm run --workspace frontend dev",
    "build": "npm run --workspace frontend build",
    "lint": "npm run --workspace frontend lint",
    "start": "npm run --workspace frontend start",
    "typecheck": "npm run --workspace frontend typecheck",
    "mcp:maps": "npm run --workspace @orbit/mcp-google-maps start",
    "mcp:sheets": "npm run --workspace @orbit/google-sheets-mcp start"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "keywords": [
    "orbit",
    "monorepo",
    "blink-and-build"
  ],
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.3"
  }
}
